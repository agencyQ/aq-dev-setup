{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "imageTemplateName": {
      "type": "string"
    },
    "api-version": {
      "type": "string"
    },
    "svclocation": {
      "type": "string"
    }
  },
  "variables": {},
  "resources": [
    {
      "name": "[parameters('imageTemplateName')]",
      "type": "Microsoft.VirtualMachineImages/imageTemplates",
      "apiVersion": "[parameters('api-version')]",
      "location": "[parameters('svclocation')]",
      "dependsOn": [],
      "tags": {
        "imagebuilderTemplate": "win11multi",
        "userIdentity": "enabled"
      },
      "identity": {
        "type": "UserAssigned",
        "userAssignedIdentities": {
          "/subscriptions/97acfce6-88e7-49dd-8e29-db06a097a787/resourcegroups/dev-workstations/providers/Microsoft.ManagedIdentity/userAssignedIdentities/aibIdentity1678550549": {}
        }
      },
      "properties": {
        "buildTimeoutInMinutes": 100,
        "vmProfile": {
          "vmSize": "Standard_DS3_v2",
          "osDiskSizeGB": 256
        },
        "source": {
          "type": "PlatformImage",
          "publisher": "MicrosoftWindowsDesktop",
          "offer": "Windows-11",
          "sku": "win11-22h2-avd",
          "version": "latest"
        },
        "customize": [
          {
            "type": "PowerShell",
            "name": "Install Choco and Vscode",
            "inline": [
              "Set-ExecutionPolicy Bypass -Scope Process -Force; [System.Net.ServicePointManager]::SecurityProtocol = [System.Net.ServicePointManager]::SecurityProtocol -bor 3072; iex ((New-Object System.Net.WebClient).DownloadString('https://community.chocolatey.org/install.ps1'))",
              "Set-ExecutionPolicy Bypass -Scope Process -Force; [System.Net.ServicePointManager]::SecurityProtocol = [System.Net.ServicePointManager]::SecurityProtocol -bor 3072; iex ((New-Object System.Net.WebClient).DownloadString('https://raw.githubusercontent.com/agencyQ/aq-dev-setup/main/Scripts/removeDefaultApps.ps1'))",
              "Set-ExecutionPolicy Bypass -Scope Process -Force; [System.Net.ServicePointManager]::SecurityProtocol = [System.Net.ServicePointManager]::SecurityProtocol -bor 3072; iex ((New-Object System.Net.WebClient).DownloadString('https://raw.githubusercontent.com/agencyQ/aq-dev-setup/main/Scripts/baseSetup.ps1'))",
              "Set-ExecutionPolicy Bypass -Scope Process -Force; [System.Net.ServicePointManager]::SecurityProtocol = [System.Net.ServicePointManager]::SecurityProtocol -bor 3072; iex ((New-Object System.Net.WebClient).DownloadString('https://raw.githubusercontent.com/agencyQ/aq-dev-setup/main/Scripts/nodeDevelopment.ps1'))",
              "Set-ExecutionPolicy Bypass -Scope Process -Force; [System.Net.ServicePointManager]::SecurityProtocol = [System.Net.ServicePointManager]::SecurityProtocol -bor 3072; iex ((New-Object System.Net.WebClient).DownloadString('https://raw.githubusercontent.com/agencyQ/aq-dev-setup/main/Scripts/visualStudio.ps1'))"
            ]
          }
        ],
        "distribute": [
          {
            "type": "SharedImage",
            "galleryImageId": "/subscriptions/97acfce6-88e7-49dd-8e29-db06a097a787/resourceGroups/dev-workstations/providers/Microsoft.Compute/galleries/devboxGallery/images/aqStandardDevImageDef",
            "runOutputName": "aibCustWinManImg01",
            "artifactTags": {
              "source": "azureVmImageBuilder",
              "baseosimg": "win11multi"
            },
            "replicationRegions": ["eastus2", "eastus"]
          }
        ]
      }
    }
  ]
}
